<view class="content"><view class="content-picker"><picker class="picker picker-school" mode="multiSelector" value="{{schoolIndex}}" range="{{schoolArray}}" data-event-opts="{{[['columnchange',[['choiseCollege',['$event']]]],['change',[['changeCollege',['$event']]]]]}}" bindcolumnchange="__e" bindchange="__e"><view class="picker-text school-text">{{collegeName}}</view><image class="picker-img" src="/static/icon/arr_down.png"></image></picker><picker class="picker picker-course" mode="multiSelector" value="{{classIndex}}" range="{{classArray}}" data-event-opts="{{[['columnchange',[['choiseClass',['$event']]]],['change',[['changeClass',['$event']]]]]}}" bindcolumnchange="__e" bindchange="__e"><view class="picker-text course-text">{{className}}</view><image class="picker-img" src="/static/icon/arr_down.png"></image></picker></view><scroll-view class="content-main" scroll-y="true" data-event-opts="{{[['scrolltolower',[['addData',['$event']]]]]}}" bindscrolltolower="__e"><block wx:for="{{AllData}}" wx:for-item="item" wx:for-index="__i0__" wx:key="classId"><view data-event-opts="{{[['tap',[['classDetailsFunc',['$0'],[[['AllData','classId',item.classId,'classId']]]]]]]}}" class="content-list" bindtap="__e"><view class="list"><image class="list-img" mode="aspectFill" src="{{'https://'+item.iconPath}}"></image><view class="list-main"><view class="list-title">{{item.title}}</view><view class="list-main-content"><text>线上</text><image class="list-main-img list-right" src="/static/time.png"></image><text>{{item.updateTime}}</text></view><view class="list-main-content"><image class="list-main-img" src="/static/bm.png"></image><text class="text-color">{{item.applyNum}}</text><text>人报名</text></view><view class="list-main-content"><text class="text-color">$</text><text class="text-color">{{item.coursePrice}}</text></view></view></view></view></block><block wx:if="{{getMoreData}}"><view class="data">正在加载数据...</view></block><block wx:else><view class="data">没有更多数据了~</view></block></scroll-view></view>