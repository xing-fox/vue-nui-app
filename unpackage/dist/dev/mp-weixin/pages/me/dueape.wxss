.content {
  line-height: 1;
  min-height: 100vh;
  padding-top: 20rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-bottom: 20rpx;
  background: #f5f5f5;
}
.content .main {
  padding-top: 40rpx;
  padding-left: 25rpx;
  padding-right: 25rpx;
  padding-bottom: 40rpx;
  background: #fff;
  text-align: center;
}
.content .main .logo {
  width: 269rpx;
  height: 72rpx;
  margin-bottom: 30rpx;
}
.content .main .text {
  font-size: 26rpx;
  line-height: 40rpx;
  text-indent: 60rpx;
  margin-bottom: 20rpx;
  text-align: left;
}
.content .main .label {
  font-weight: bold;
  margin-top: 40rpx;
  margin-bottom: 20rpx;
  padding-left: 20rpx;
  text-align: left;
  position: relative;
}
.content .main .label::after {
  content: '';
  width: 10rpx;
  height: 25rpx;
  background: #ff9704;
  vertical-align: middle;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  margin: auto 0;
}

